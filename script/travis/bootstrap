#!/bin/bash

# featureブランチのときのみmakepkgを行う
# master/developブランチではmiscの結果を見てテスト結果を返す

source ./lib/utils.sh

if [ is_feature_branch = true ]; then
  ./script/travis/feature || exit 1
elif [ is_master_or_develop_branch = true ]; then
  ./script/travis/check_packages || exit 1
else
  echo "nothing to do"
fi

